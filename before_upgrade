/def -Fp5 -mregexp -t'^([A-Za-z,-:\' ]+)\'s gender is: ([a-z]+)$' kya_gender = /quote -S /unset `/listvar -s kya_*%; /set kya_short=%{P1}%; /set kya_gender=$[tolower({P2})]
/def -Fp5 -mregexp -t'^       race:                   ([a-z ]+)$' kya_race = /set kya_race=$[tolower({P1})]
/def -Fp5 -mregexp -t'^       money:                  (\\d+)$' kya_money = /set kya_money=%{P1}
/def -Fp5 -mregexp -t'^       armour class            (\\d+)$' kya_armour_class = /set kya_armour_class=%{P1}
/def -Fp5 -mregexp -t'^       weapon class:           (\\d+)$' kya_weapon_class = /set kya_weapon_class=%{P1}
