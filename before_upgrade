/def from_expstr = /let _result=0%; /let i=1%; /let j=$(/length %{*})%; /while (i <= j) /let _part=$(/nth %{i} %{*})%; /let _left=$[substr(_part, 0, -1)]%; /let _right=$[substr(_part, -1)]%; /if (_right =~ 'k') /test _result += _left * 1000%; /elseif (_right =~ 'M') /test _result += _left * 1000000%; /elseif (_right =~ 'G') /test _result += _left * 1000000000%; /elseif (_right =~ 'T') /test _result += _left * 1000000000000%; /else /test _result += _part%; /endif%; /test ++i%; /done%; /result _result
/def to_nth = /let _num=$[trunc({1})]%; /let _2=$[substr(_num, -2)]%; /let _1=$[substr(_num, -1)]%; /if (_1 == 1 & _2 != 11) /let _end=st%; /elseif (_1 == 2 & _2 != 12) /let _end=nd%; /elseif (_1 == 3 & _2 != 13) /let _end=rd%; /else /let _end=th%; /endif%; /result strcat(_num, _end)
