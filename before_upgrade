/def get_paladin_rank = /if ({1} == 1) /result 500%; /elseif ({1} == 2) /result 1303%; /elseif ({1} == 3) /result 2591%; /elseif ({1} == 4) /result 4660%; /elseif ({1} == 5) /result 7980%; /elseif ({1} == 6) /result 13310%; /elseif ({1} == 7) /result 21867%; /elseif ({1} == 8) /result 35604%; /elseif ({1} == 9) /result 57655%; /elseif ({1} == 10) /result 93054%; /elseif ({1} == 11) /result 149879%; /elseif ({1} == 12) /result 241102%; /elseif ({1} == 13) /result 387542%; /elseif ({1} == 14) /result 622623%; /elseif ({1} == 15) /result 1000000%; /elseif ({1} == 16) /result 1413342%; /elseif ({1} == 17) /result 1866076%; /elseif ({1} == 18) /result 2361957%; /elseif ({1} == 19) /result 2905097%; /elseif ({1} == 20) /result 3500000%; /elseif ({1} == 21) /result 4151598%; /elseif ({1} == 22) /result 4865296%; /elseif ({1} == 23) /result 5647011%; /elseif ({1} == 24) /result 6503225%; /elseif ({1} >= 25) /result 7441038%; /else /result 0%; /endif
