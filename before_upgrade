/def cpl = /check_effects %{*}
/def ce = /check_effects %{*}
/def check_effects_party = /say -d'party' -x -c'blue' -- --------------------------------------------------------------%; /python effects.inst.forall('/say -d"party" -x -c"%%(color)s" -- %%(name)-35s %%(count)10s %%(status)15s', keys='$(/escape ' %{effects_list})', online=True, offline=True)%; /say -d'party' -x -c'blue' -- --------------------------------------------------------------
